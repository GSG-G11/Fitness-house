{
  "name": "fitness-house",
  "version": "1.0.0",
  "description": "a website that give the user the ability to search about Gyms suits him",
  "main": "index.js",
  "scripts": {
    "test": "cross-env DB_BUILD=true NODE_ENV=test jest --runInBand",
    "lint-fix": "npm run lint-server && cd client && npm run lint && cd ..",
    "prepare": "is-ci || npx husky install",
    "dev": "cross-env NODE_ENV=dev nodemon server/index.ts",
    "dev:client": "cd client && npm start",
    "start": "NODE_ENV=production node ./dist",
    "lint-server": "eslint --fix --ext .js,.ts server/",
    "db:build": "cross-env DB_BUILD=true NODE_ENV=dev ts-node ./server/database/fakeData/index.ts",
    "db:build-production": "cross-env DB_BUILD=true NODE_ENV=production ts-node ./server/database/fakeData/index.ts",
    "heroku-postbuild": " cd client && npm i && npm run build",
    "tsc": "tsc",
    "postinstall": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSG-G11/Fitness-house.git"
  },
